{
    "presets": [
        ["env"],
        "react"
    ],
    "plugins": [
        ["module-resolver", {
            "root": ["./src"]
        }],
        ["babel-plugin-styled-components", {
            "displayName": false,
            "minify": false
        }],
        "transform-export-extensions",
        "react-hot-loader/babel",
        "transform-object-rest-spread",
        "transform-class-properties",
        "array-includes"
    ]
}